<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="MyTextView">
        <attr name="MyText" format="string"/>
        <attr name="MyTextColor" format="color"/>
        <attr name="MyTextSize" format="dimension"/>
        <attr name="maxLength" format="integer"/>
        <attr name="inputType">
            <enum name="number" value="1"/>
            <enum name="text" value="2"/>
            <enum name="password" value="3"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="QQStepView">
        <attr name="outerColor" format="color"/>
        <attr name="innerColor" format="color"/>
        <attr name="borderWidth" format="dimension"/>
        <attr name="stepTextSize" format="dimension"/>
        <attr name="stepTextColor" format="color"/>
    </declare-styleable>

    <declare-styleable name="ColorTrackTextView">
        <attr name="originColor" format="color"/>
        <attr name="changeColor" format="color"/>
    </declare-styleable>

    <declare-styleable name="ProgressBar">
        <attr name="innerBackground" format="color"/>
        <attr name="outerBackground" format="color"/>
        <attr name="roundWidth" format="dimension"/>
        <attr name="progressTextSize" format="dimension"/>
        <attr name="processTextColor" format="color"/>
    </declare-styleable>

    <declare-styleable name="RatingBar">
        <attr name="startNormal" format="reference"/>
        <attr name="startFocus" format="reference"/>
        <attr name="gradeNumber" format="integer"/>
        <attr name="distanceGap" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="LetterSideBar">
        <attr name="letterGap" format="dimension"/>
        <attr name="paintNormalColor" format="color"/>
        <attr name="paintChangeColor" format="color"/>
        <attr name="paintSize" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="TagCloudView">
        <attr name="tcvBackground" format="reference"/>
        <attr name="tcvTextColor" format="color"/>
        <attr name="tcvBorder" format="dimension"/>
        <attr name="tcvTextSize" format="integer"/>
        <attr name="tcvBorderItem" format="dimension"/>
        <attr name="tcvItemBorderVertical" format="dimension"/>
        <attr name="tcvItemBorderHorizontal" format="dimension"/>

        <attr name="tcvSingleLine" format="boolean"/>
        <attr name="tcvShowEndText" format="boolean"/>
        <attr name="tcvShowRightImg" format="boolean"/>
        <attr name="tcvEndText" format="string"/>
        <attr name="tcvRightResId" format="reference"/>
        <attr name="tcvCanTagClick" format="boolean"/>

        <attr name="tcvTagResId" format="reference"/>
    </declare-styleable>

    <declare-styleable name="SlideBottomPanel">
        <attr name="sbp_background_layout" format="reference"/>
        <attr name="sbp_title_height_no_display" format="dimension"/>
        <attr name="sbp_panel_height" format="dimension"/>
        <attr name="sbp_boundary" format="boolean"/>
        <attr name="sbp_move_distance_trigger" format="dimension"/>
        <attr name="sbp_animation_duration" format="integer"/>
        <attr name="sbp_fade" format="boolean"/>
        <attr name="sbp_hide_panel_title" format="boolean"/>
    </declare-styleable>
</resources>